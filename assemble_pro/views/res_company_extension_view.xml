<?xml version="1.0" encoding="utf-8"?>
<odoo>

<record id="res_company_extension_form" model="ir.ui.view">
	<field name="name">res.company.extension.form</field>
	<field name="model">res.company</field>
	<field name="inherit_id" ref="base.view_company_form" />
	<field name="arch" type="xml">

		<field name="website" position="after" >
			<field name="customer_company_name"/>
			<field name="company_range"/>
			<field name="division"/>
			<field name="commissionrate"/>
		</field>
		<field name="parent_id" position="after" >
			<field name="vendor_code" />
			<field name="vendor_number"/>
			<field name="sac_code" />
			<field name="gstin_no"/>
		</field>
		
	</field>
</record>

</odoo>