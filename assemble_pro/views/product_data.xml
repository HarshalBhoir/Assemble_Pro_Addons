<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">

<!-- Resource: product.uom.categ -->
<record id="product_uom_categ_rental" model="product.uom.categ">
	<field name="name">Rental</field>
</record>


<!-- Resource: product.uom -->
<record id="product.product_uom_week" model="product.uom">
	<field name="category_id" ref="product.uom_categ_wtime"/>
	<field name="name">Week(s)</field>
	<field name="factor_inv" eval="7"/>
	<field name="uom_type">bigger</field>
</record>

<record id="product.product_uom_month" model="product.uom">
	<field name="category_id" ref="product.uom_categ_wtime"/>
	<field name="name">Month(s)</field>
	<field name="factor_inv" eval="30"/>
	<field name="uom_type">bigger</field>
</record>

</odoo>