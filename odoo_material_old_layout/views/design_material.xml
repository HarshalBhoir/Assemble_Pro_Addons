<template id="odoo_material_old_layout.menu" name="Material Menu" inherit_id="web.menu">
    <xpath expr="//ul[@class='nav navbar-nav navbar-left oe_application_menu_placeholder']" position="replace">               
       <div class="oe_application_menu_placeholder">
           <t t-foreach="menu_data['children']" t-as="menu">
               <ul id="main-menu" class="nav navbar-nav navbar-left tnav hide" t-att-data-menu-parent="menu['id']">
                   <li class="app-name">
                       <span class="oe_menu_text">
                           <t t-esc="menu['name']"/>
                       </span>
                   </li>
                   <li><t t-call="odoo_material_old_layout.menu_secondary_submenu"/></li>
               </ul>
           </t>
       </div>
   </xpath>
    
    <xpath expr="//ul[@class='nav navbar-nav navbar-right oe_systray']" position="after">
        <a class="oe_logo dbt_logo nav navbar-nav navbar-right" t-att-href="'/web/?debug' if debug else '/web'">
            <img src="/web/binary/company_logo"/>
        </a>
     </xpath>
</template>

